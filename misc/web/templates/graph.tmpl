    {{define "content"}}
    <div id="app">
      <div class="container-fluid">
        <div class="row">
          <div class="col-xs-8">
            <br>
            <div class="dropdown">
              <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                <span style="font-size:32px">{{"{{"}} selectedView {{"}}"}} Graph <span class="caret"></span></span>
              </button>
              <ul class="dropdown-menu">
                <li><a href="#" v-on:click="selectedView = 'Dandelion'">Dandelion Graph</a></li>
                <li><a href="#" v-on:click="selectedView = 'Host-Centric'">Host-Centric Graph</a></li>
                <li><a href="#" v-on:click="selectedView = 'VLAN-Centric'">VLAN-Centric Graph</a></li>
              </ul>
            </div>

            <br>

            <host-centric-graph v-if="selectedView == 'Host-Centric'"></host-centric-graph>
            <dandelion-graph v-if="selectedView == 'Dandelion'"></dandelion-graph>
            <vlan-centric-graph v-if="selectedView == 'VLAN-Centric'" v-on:vlan-selected="vlanNodeClicked($event)"></vlan-centric-graph>
          </div>

          <div class="col-xs-4">
            <vlan-list-group ref="list"></vlan-list-group>
          </div>
        </div>
      </div>
    </div>

